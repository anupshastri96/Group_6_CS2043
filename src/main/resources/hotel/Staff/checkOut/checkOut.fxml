<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>


<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="hotel.UI.Staff.checkOut.checkOutController"
      spacing="10" alignment="CENTER">

    <children>
        <Text text="" fx:id="checkOutText"/>
        <Button fx:id="checkOutButton" text="Check out" onAction="#processCheckout"/>
        <Button fx:id="cancelButton" text="Cancel" onAction="#processExit"/>
    </children>
    <padding>
        <Insets top="10" right="20" bottom="20" left="20"/>
    </padding>
</VBox>