<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="hotel.UI.Staff.ModifyStay.ModifyStayOptions.extendStayController"
      spacing="10" alignment="CENTER">

    <children>
        <Text text="Please select desired date below:" fx:id="extendText"/>
         <DatePicker fx:id="extendCheckout"/>
        <Button text="Enter" onAction="#processExtendStay"/>
        <Button text="Exit" onAction="#processExit"/>
    </children>
    <padding>
        <Insets top="10" right="20" bottom="20" left="20"/>
    </padding>
</VBox>